package day10;
@FunctionalInterface
interface Sum{
	void SumOfN();
	
}
public class SumOfNatural {

	public static void main(String[] args) {
Sum sum = new Sum() {
			
			@Override
			public void SumOfN() {
				int i, num = 15,sum = 0;
				for(i = 1; i<= num; ++i)
				{
					// adding the value of i into sum variable
					sum = sum + i;
				}
				// prints the sum
				System.out.println("Sum of First 15 Natural Nnumbers is =" + sum);
				
			}
		};
		sum.SumOfN();
	}
}
